}
#selCategWrap.selCategHei_wh{
	height: 126px;
}
.addCategBox{
	position:absolute;
	left:0px;
	background:#fff;
	z-index: 3;
	display: none;
	border-radius:0 0 4px 4px;
}
.addCategBox.addCategBoxHei_pro{
	height: 320px;
	top:-380px;
}
.addCategBox.addCategBoxHei_un{
	height: 452px;
	top:-512px;
}
.addCategBox.addCategBoxHei_wh{
	height: 188px;
	top:-248px;
}
#selCategUl{
	position:absolute;
	left:0px;
	top:0px;
}
.selCategBot{
	height: 45px;
	margin-top:8px;
}
.selCategBot a{
	display: inline-block;
	height: 100%;
	font-size: 18px;
	text-align: center;
	line-height: 45px;
	border-radius: 4px;
}
.selCategBot a.addBtn{
	background:#68c7ec;
	color: #fff;
	float: left;
}
.selCategBot a.addBtn.btn1{
	width: 130px;
	margin:0 10px 0 114px;
}
.selCategBot a.addBtn.btn2{
	width: 100px;
}
.btnMargL{
	margin-left:175px;
}
.selCategBot a.addBtn:hover,
.addBotInner a.saveBtn:hover{
	background:#00a3e4;
}
.selCategBot a.cancelBtn{
	width: 100px;
	background:#dbdbdb;
	margin-left:10px;
	color: #666;
}
.selCategBot a.cancelBtn:hover,
.addBotInner a.cancel:hover{
	text-decoration: underline;
}
.addBotInner{
	width: 100%;
	height: 50px;
	position:absolute;
	left:0px;
	bottom:0px;
	border-radius:0 0 6px 6px;
}
.addBotInner a{
	width: 100px;
	height: 40px;
	line-height: 40px;
	display: inline-block;
	text-align: center;
	font-size: 18px;
	border-radius: 4px;
}
.addBotInner a.saveBtn{
	background:#68c7ec;
	color:#fff;
	margin-left:180px;
}
.addBotInner a.cancel{
	background:#dbdbdb;
	margin-left:10px;
	color:#666;
}
