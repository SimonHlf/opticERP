}
.classifyA{
	width: 35px;
	height: 35px;
	position:absolute;
	right:0px;
	top:0px;
}
.classifyIcon{
	color:#00a3e4;
	font-size: 34px;
}
.addBot{
	width: 100%;
	height: 50px;
	margin-top:26px;
}
.addBot a{
	width: 100%;
	height: 100%;
	line-height: 48px;
	background:#68c7ec;
	display: inline-block;
	font-size:18px;
	color:#fff;
	text-align: center;
	border-radius:0 0 6px 6px;
}
.addBot a:hover{
	background:#00a3e4;
}
.checkCategLayer,
.addBankBox{
	width: 100%;
	height: 100%;
	position: absolute;
	left:0px;
	top:0px;
	z-index:3;
	background:#fff;
	opacity:0;
	filter:alpha(opacity=0);
	border-radius:6px;
	display: none;
	overflow: hidden;
}
.checkCategLayer p.nowCategP,
.addBankBox p.nowBankP{
	position: absolute;
	top:30px;
	left:22px;
	font-size:14px;
	color:#00a3e4;
	z-index: 2;
}
#selCategWrap,
.addCategBox,
.bankDiv{
	width: 100%;
	margin-top:60px;
	color: #666;
	overflow: hidden;
}
#selCategWrap,
.bankDiv{
	position:relative;
	z-index: 2;
}
#selCategWrap.selCategHei_pro{
	height: 260px;
}
#selCategWrap.selCategHei_un,
.bankDiv{
