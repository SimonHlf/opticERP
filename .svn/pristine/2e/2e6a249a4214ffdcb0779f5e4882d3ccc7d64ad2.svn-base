@charset "utf-8";
/* 搜索框公共 */
.searWrap{
	width: 100%;
	height: 69px;
	background:#eaeaea;
	border-bottom:1px solid #d8d8d8;
}
.searBox{
	height: 100%;
}
.searBox p{
	font-size: 18px;
	color: #666;
	margin-top:20px;
}
.searBox p a{
	display: inline-block;
	border:1px dotted #00a3e4;
	padding:0 10px;
	color:#666;
	position: relative;
}
.addCategBtn,
.addCategUnitBtn{
	height: 39px;
	line-height: 39px;
	background:#fff;
	display: inline-block;
	font-size: 16px;
	color: #666;
	margin:14px 0 0 0px;
}
.addCategBtn{
	width: 122px;
}
.addCategUnitBtn{
	width: 150px;
	margin-left:15px;
}
.addCategBtn:hover,
.addCategUnitBtn:hover{
	background: #00a3e4;
	color:#fff;
}
.addCategBtn:hover .addCaIcon,
.addCategUnitBtn:hover .addCaIcon{
	color:#fff;
}
.addCaIcon{
	color: #00a3e4;
	font-size: 26px;
	margin:0 3px 0 15px;
}
.searchDiv{
	width: 220px;
	height: 38px;
	background:#fff;
	border:1px solid #d8d8d8;
	position: relative;
}
.margL_categ{
	margin:14px 0px 0 30px;
}
.margL_pro{
	margin:14px 0px 0 5px;
}
.searInp{
	width: 170px;
	border:none;
	height:20px;
	padding:9px 0 9px 5px;
	background:none;
	font-size:16px;
	color:#999;
}
.searA{
	width: 45px;
	height: 38px;
	display: inline-block;
	position:relative;
}
.searIcon{
	color: #00a3e4;
	font-size: 24px;
	position:absolute;
	right:10px;
	top:2px;
}
.searA:hover{
	background:#00a3e4;
}
.searA:hover .searIcon{
	color:#fff;
}
.closeIcon,
.closeIcon_1,
.closeIcon_2{
	position:absolute;
	top:-2px;
	font-size:32px;
	cursor: pointer;
}
.closeIcon_3{
	color: #f00;
	font-size: 18px;
}
.closeIcon,
.closeIcon_1{
	color:#666;
}
.closeIcon_2{
	color:#999;
	right:-6px;
}
.closeIcon{
	right:5px;
}
.closeIcon_1{
	left:5px;
}
.closeIcon:hover,
.closeIcon_1:hover,
.closeIcon_2:hover{
	color:#f00;
	-webkit-transform: rotate(180deg);
	-moz-transform: rotate(180deg);
	-ms-transform: rotate(180deg);
	transform: rotate(180deg);
}
/* 类别公共层 */
.margT_prod{
	margin-top:20px;
	margin-bottom:10px;
}
.categBox{
	border:1px solid #dbdbdb;
	box-sizing: border-box;
	color: #666;
}
.categHei_proInfo{
	height: 150px;
}
.categHei_unit{
	height: 80px;
}
.categBox .titP{
	width: 80px;
	height: 100%;
	background:#00a3e4;
	text-align: center;
	font-size: 14px;	
	color: #fff;
}
.categHei_proInfo .titP{
	line-height: 150px;
}
.categHei_unit .titP{
	line-height: 80px;
}
.categUlBox{
	height: 100%;
	position:relative;
	overflow: hidden;
}
.noAddCateg{
	position: absolute;
	left:50%;
}
.margL1{
	margin-left:-196px;
	top:30px;
}
.margL2{
	margin-left:130px;
	top:80px;
}
.margL3{
	margin-left:-208px;
	top:8px;
}
.margL4{
	margin-left:118px;
	top:150px;
}
.margL5{
	margin-left:130px;
	top:6px;
}
.margL6{
	margin-left:130px;
	top:35px;
}
.categUlBox ul{
	position:absolute;
	left:0px;
	top:0px;
	padding-top:10px;
}
.categUlBox li,
#selCategWrap li{
	width: 100px;
	max-width: 100px;
	height: 35px;
	line-height: 35px;
	float: left;
	text-align: center;
	font-size: 14px;
	cursor: pointer;
}
.categUlBox li{
	margin:0 0 10px 10px;
}
#selCategWrap li{
	margin:0 0 10px 8px;
}
.categUlBox li:hover,
#selCategWrap li:hover{
	background:#00a3e4;
	color:#fff;
}
#selCategWrap li.active{
	background:#00a3e4;
	color: #fff;
}
.categUlBox li.active:hover{
	color:#fff;
}
.categUlBox li.active{
	background:#00a3e4;
	color: #fff;
}
/* 列表公共层 */
.listTit,
.listCon{
	width: 100%;
	font-size:14px;
	position:relative;
}
.listTit{
	font-weight: bold;
	height:40px;
	line-height: 40px;
	background: #eaeaea;
}
.listCon li{
	height: 49px;
	line-height: 49px;
	border-bottom:1px dashed #dbdbdb;
	vertical-align: middle;
}
.listBg{
	background:#f3f3f3;
}
.listCon li p a{
	color:#00a3e4;
	font-weight: bold;
}
.listCon li p a:hover{
	color:#f90;
}
.listTit li,
.listCon li p{
	float:left;
	text-align: center;
}
.listCon img{
	position:absolute;
	left:50%;
	margin-left:-129px;
	top:100px;
}
/* 暂无记录 */
.noInfoDiv{
	width: 230px;
	height: 355px;
	display: none;
	color:#666;
	font-size:16px;
	text-align: center;
	margin:100px auto 0px;
}
.noInfoDiv img{
	opacity: 0.9;
	filter:alpha(opacity=90);
}
/* 模拟滚动条 */
.parScroll,
.sonScroll{
	position:absolute;
	right:0px;
	top:0px;
	border-radius:10px;
}
.scrollWid{
	width: 10px;
}
.scrollWid1{
	width: 5px;
}
.parScroll{
	height: 100%;
	background:#999;
}
.sonScroll{
	height: 50px;
	background:#d8d8d8;
}
/* 增加/编辑商品、增加/编辑业务往来单位公共弹窗 */
.addNewProDiv{
