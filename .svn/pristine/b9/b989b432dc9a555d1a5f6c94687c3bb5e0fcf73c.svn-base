/* 类别类型 */
.categStyLab{
	float: left;
	display: inline-block;
	height: 100%;
	width: 80px;
	font-size: 14px;
	position: relative;
	text-indent: 15px;
}
.categStyLab input{
	width: 100%;
	height: 100%;
	position: absolute;
	left:0px;
	top:0px;
	z-index: 2;
	opacity: 0;
	filter:alpha(opacity=0);
	cursor: pointer;
}
.categStyLab em{
	font-style: normal;
}
.categStyLab .cirSpan{
	width: 20px;
	height: 20px;
	border:1px solid #999;
	border-radius: 50%;
	position: absolute;
	right:10px;
	top:12px;
	/*background:#00a3e4;*/
}
.categStyLab i.choiceAct{
	color: #00a3e4;
	font-size: 18px;
	position: absolute;
	left:35px;
	top:1px;
}