@charset "utf-8";
/* 头部搜索--st */
.searWrap{
	width: 100%;
	height: 69px;
	background:#eaeaea;
	border-bottom:1px solid #d8d8d8;
}
.searBox{
	height: 100%;
}
.searBox p{
	height:40px;
	line-height:40px;
	font-size:14px;
	color:#666;
	margin-top:15px;
}
.searBox p.margLP{
	margin-left:15px;
}
.searInp{
	height:20px;
	border:1px solid #d8d8d8;
	background:#fff;
	padding:9px 0 9px 5px;
	font-size:16px;
}
.inpWid1{
	width: 160px;
	color:#999;
}
.inpWid2{
	width: 80px;
	color:#666;
}
.searBox p label{
	display:inline-block;
	position:relative;
	text-indent:28px;
}
.searBox p label:nth-of-type(2n){
	margin-left:8px;
}
.searBox p label input{
	width:100%;
	height:100%;
	position:absolute;
	left:0px;
	top:0px;
	z-index:2;
	cursor:pointer;
	opacity:0;
	filter:alpha(opacity=0);
}
.searBox p label .cirSpan{
	width:22px;
	height:22px;
	border-radius:50%;
	position:absolute;
	left:0px;
	border:1px solid #999;
	top:8px;
}
.searBox p label i{
	position:absolute;
	font-size:18px;
	left:-25px;
	top:1px;
}
.searBox p label.active,
.searBox p label i{
	color:#00a3e4;
}
.searBox p label.active .cirSpan{
	border:1px solid #00a3e4;
}
.yearSpan{
	margin:0 2px;
}
.monthSpan{
	margin-left:2px;
}
.searABtn{
	width:100px;
	height:40px;
	line-height:40px;
	background:#68c7ec;
	color:#fff;
	text-indent:28px;
	font-size:16px;
	margin:15px 0 0 10px;
	position:relative;
}
.searABtn:hover{
	
	background:#00a3e4;
}
.searIcon{
	font-size:24px;
	color:#fff;
	position:absolute;
	right:10px;
}
/* 头部搜索--end */

/* 数据列表层--st */
.dataLisTit{
	width:100%;
	height:40px;
	line-height:40px;
	background:#eaeaea;
	font-weight:bold;	
	margin-top:20px;
	color:#666;
	font-size:14px;
}
.dataLisTit li,
.dataLisCon li p{
	float:left;
	text-align:center;
}
.dataLisCon li{
	height: 49px;
	line-height: 49px;
	border-bottom:1px dashed #dbdbdb;
	vertical-align: middle;
		font-size:14px;
	color:#666;
}
.dataLisCon li p a{
	color:#00a3e4;
}
.dataLisCon li p a:nth-of-type(2n){
	margin-left:10px;
}
.dataLisCon li p a:hover{
	color:#f90;
}
.lisWid1{
	width:180px;
}
.lisWid3{
	width:60px;
}
.lisWid4{
	width:120px;
}
/* 查看详情下数据层 */
.detailData{
	width:1100px;
	height:920px;
	background:#fff;
	position:absolute;
	left:50%;
	margin-left:-550px;
	top:0px;
	z-index:1000;
	overflow:hidden;
	display:none;
}
.detailTit{
	font-size:28px;
	color:#666;
	text-align:center;
	padding:10px 0px 5px 0px;
	position:relative;
}
.detailTit strong{
	font-weight:normal;
}
.closeDetTitIcon{
	font-size:46px;
	color:#f00;
	position:absolute;
	right:0px;
	top:-12px;
	cursor:pointer;
}
.smNav{
	height:39px;
	line-height:40px;
	border-bottom:1px solid #ccc;
}
.smNav p{
	width:120px;
	height:38px;
	float:left;
	font-size:18px;
	color:#666;
	border:1px solid #ccc;
	border-bottom:none;
	text-align:center;
	margin-left:20px;
	cursor:pointer;
}
.smNav p:hover{
	color:#00a3e4;
}
.smNav p.active{
	height:39px;
	background:#fff;
	color:#00a3e4;
}
.routeDivBox{
	width:100%;
	margin:20px auto 0px;
	position:relative;
	overflow:hidden;
}
.chartDivBox{
	display:none;
}
#routeDivSon{
	width:870px;
	position:absolute;
	left:50%;
	margin-left:-435px;
	top:0px;
}
.comRouteDiv{
	width:180px;
	color:#666;
	position:relative;
	margin-left:15px;
}
.posDiv{
	margin-right:51px;
}
.comRouteDiv .prodProc{
	text-align:center;
	font-size:22px;
	margin-bottom:8px;
}
.comConDiv{
	border:1px solid #ccc;
	font-size:16px;
	padding-bottom:4px;
	border-radius:4px;
	position:relative;
}
.arrowSpan{
	width:21px;
	height:11px;
	position:absolute;
	left:50%;
	margin-left:-14px;
	top:-11px;
	background:#fff url(../images/triangle.png) no-repeat;
	z-index:1;
}
.comConDiv p{
	margin:8px 0 0 12px;
}
.comConDiv a{
	display:inline-block;
	width:100%;
	text-indent:112px;
	font-size:14px;
	color:#68c7ec;
}
.comConDiv a:hover{
	color:#00a3e4;
}
.horComLine{
	width:100px;
	margin-left:15px;
	height:179px;
	position:relative;
}
.vertComLine{
	width:100%;
	float:left;
	height:50px;
	margin:10px 70px 0 0px;
	position:relative;
}
.arrowLine{
	background:#ccc;
	position:absolute;
}
.horzLine{
	width:100px;
	height:2px;
	left:0px;
	top:50%;
	margin-top:30px;
}
.vertLine{
	width:2px;
	height:50px;
	top:0px;
}
.posVerLineL{
	left:102px;
}
.posVerLineR{
	right:140px;
}
.comTriSpan{
	position:absolute;
	width:0px;
	height:0px;
	border-width:8px;
}
.triSpanR{
	top:-7px;
	right:-8px;
	border-color:transparent transparent transparent #ccc;
	border-style:dashed dashed dashed solid;	
}
.triSpanL{
	top:-7px;
	left:-8px;
	border-color:transparent #ccc transparent transparent;
	border-style:dashed solid dashed dashed;
}
.triSpanB{
	bottom:-8px;
	left:-7px;
	border-color:#ccc transparent transparent transparent;
	border-style:solid dashed dashed dashed;
}
#scrollPar_1,
#scrollSon_1{
	width:10px;
	border-radius:5px;
	position:absolute;
	
}
#scrollPar_1{
	right:5px;
	top:115px;
}
#scrollSon_1{
	left:0px;
	top:0px;
	height:50px;
	background:#ccc;
}
/* 查看每个工序领料损耗详情 */
.viewDetLay,
.viewDetailDiv{
	position:absolute;
	display:none;
}
.viewDetLay{
	width:100%;
	height:100%;
	left:0px;
	top:0px;
	background:#fff;
	opacity:0.8;
	filter:alpha(opacity=80);
	z-index:10;
}
.viewDetailDiv{
	width:660px;
	height:350px;
	background:#fff;
	position:absolute;
	left:50%;
	margin-left:-330px;
	top:50%;
	margin-top:-175px;
	z-index:11;
	border:1px solid #ccc;
	border-radius:4px;
}
.viewTit{
	height:40px;
	line-height:40px;
	background:#68c7ec;
	color:#fff;
	font-size:16px;
	text-indent:15px;
	border-radius:4px 4px 0 0;
}
.clVieDetIcon{
	float:right;
	font-size:34px;
	margin-right:5px;
	cursor:pointer;
}
.clVieDetIcon:hover{
	color:#f00;
}
.smDetaLisTit{
	height:40px;
	line-height:40px;
	color:#333;
}
.smDetailCon li{
	height:39px;
	line-height:40px;
	border-bottom:1px dashed #ccc;
}
.smDetaLisTit li,
.smDetailCon li p{
	width:110px;
	float:left;
	text-align:center;
	font-size:14px;
	color:#666;
}
#smDetailConDiv{
	height:270px;
	position:relative;
	overflow:hidden;
}
.smDetailCon{
	position:absolute;
	left:0px;
	top:0px;
}
.parScroll,
.sonScroll{
	width:5px;
	position:absolute;
	right:0px;
	border-radius:20px;
}
.parScroll{
	height:100%;
	top:80px;
}
.sonScroll{
	height:50px;
	background:#ccc;
	top:0px;
}
/* 查看详情--end */


/* 增加详情--st */
.addDetailDiv{
	width:550px;
	height:314px;
	background:#fff;
	position:absolute;
	left:50%;
	top:50%;
	margin-left:-275px;
	margin-top:-157px;
	border-radius:6px;
	z-index:1000;
	display:none;
}
.closeIcon{
	position:absolute;
	top:0px;
	right:5px;
	font-size:32px;
	color:#666;
	cursor:pointer;
}
.closeIcon:hover{
	color:#f00;
	-webkit-transform:rotate(180deg);
	-moz-transform:rotate(180deg);
	-ms-transform:roatate(180deg);
	transform:rotate(180deg);
}
.decSpan{
	width: 100px;
	height: 100px;
	border:2px solid #fff;
	background:#68c7ec;
	border-radius:50%;
	position:absolute;
	left:50%;
	margin-left:-50px;
	top:-50px;
	z-index:5;
}
.addDetIcon{
	font-size:56px;
	color:#fff;
	position:absolute;
	left:21px;
	top:10px;
}
.comAddDiv{
	width:275px;
	height:46px;
	line-height:46px;
	font-size:14px;
	color:#666;
	float:left;
	position:relative;
	margin:20px auto 0px;
}
.comAddDiv.margT{
	margin-top:60px;
}
.comAddDiv span{
	margin-left:15px;
}
.comAddDiv .comInp_add{
	width:170px;
	height:19px;
	padding:8px 0px;
	font-size:14px;
	color:#666;
	border:none;
	border-bottom:1px solid #999;
}
.comDisDiv{
	width:170px;
	height:40px;
	line-height:47px;
	border-bottom:1px solid #999;
	float:left;
}
.addBot{
	width:100%;
	height:50px;
	margin-top:26px;
}
.addBot a{
	width: 100%;
	height: 100%;
	line-height: 48px;
	background:#68c7ec;
	display: inline-block;
	font-size:18px;
	color:#fff;
	text-align: center;
	border-radius:0 0 6px 6px;
}
.addBot a:hover{
	background:#00a3e4;
}
.noRecCon{
	width:100%;
	height:150px;
	text-align:center;
	margin-top:80px;
}
.noRecCon i{
	font-size:100px;
	color:#ccc;
}
.noRecCon span{
	display:block;
	font-size:16px;
	margin-top:-20px;
	color:#666;
}
#depId{
	color:#666;
	height:30px;
	width:150px;
}
