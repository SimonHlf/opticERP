/* 浏览入库单数据层 */
.viewInsInfoDiv li.cardLi{
	width:346px;
	height:220px;
	float:left;
	margin-bottom:40px;
	position:relative;
	cursor:pointer;
}
.viewInsInfoDiv li.cardLi:nth-child(3n-1){
	margin-left:80px;
	margin-right:80px;
}
.viewInsInfoDiv li.cardLi:hover .front,
.viewInsInfoDiv li.cardLi:hover .back{
	box-shadow:0 0 20px #666;
}
.viewInsInfoDiv li div.cardDiv{
	position:relative;
	width:100%;
	height:100%;
	position:relative;
}
.shadowSpan{
	width:345px;
	height:12px;
	background:url(../images/liShadow.png) no-repeat;
	position:absolute;
	left:0px;
	bottom:-12px;
}
.back a{
	width:20px;
	height:20px;
	background:url(../images/closeIcon.png) no-repeat;
	position:absolute;
	right:0px;
	top:0px;
	z-index:4;
}
.viewInsInfoDiv li div div.front,
.viewInsInfoDiv li div div.back{
	position:absolute;
	left:0;
	top:0;
	width:100%;
	height:100%;
	-webkit-transition:all .8s ease-in-out;
	-moz-transition:all .8s ease-in-out;
	transition:all .8s ease-in-out;
	-webkit-transform-style:preserve-3d;
	-moz-transform-style:preserve-3d;
	-webkit-backface-visibility:hidden;
	-moz-backface-visiblility:hidden;
}
.viewInsInfoDiv li div div.front{
	-webkit-transform:rotateY(0deg);
	-moz-transform:rotateY(0deg);
	transform:rotateY(0deg);
	z-index:2;	
}
.viewInsInfoDiv li div div.back{
	-webkit-transform:rotateY(180deg);
	-moz-transform:rotateY(180deg);
	transform:rotateY(180deg);
	z-index:1;
}
.viewInsInfoDiv li div:hover div.front{
	z-index:1;
	-webkit-transform:rotateY(180deg);
	-moz-transform:rotateY(180deg);
	transform:rotateY(180deg);
}
.viewInsInfoDiv li div:hover div.back{
	z-index:2;
	-webkit-transform:rotateY(0deg);
	-moz-transform:rotateY(0deg);
	transform:rotateY(0deg);
}
.viewInsInfoDiv li div div.front{
	background:#f1f1f1;
}
.viewInsInfoDiv li div div.back{
	background:#f1f1f1;
}
